{
  "extends": "airbnb",
  "plugins": ["react"],
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [
      1, { "extensions": [".js", ".jsx"] }
    ],
    "no-plusplus": [0],
    "no-console": [0],
    "class-methods-use-this": [0],
    "react/forbid-prop-types": [
      1, { "forbid": ["any"] }
    ],
    "react/no-unused-prop-types": [0],
    "react/prefer-stateless-function": [0],
    "jsx-a11y/no-static-element-interactions": [0],
    "no-return-assign": [0]
  },
  "globals": {
    "fetch": true,
    "$": true
  }
}
